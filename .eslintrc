{
  "extends": ["react-app", "react-app/jest"],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "args": "all",
        "argsIgnorePattern": "^_"
      }
    ],
    "react/function-component-definition": "warn",
    "prefer-arrow-callback": "warn",
    "arrow-body-style": ["warn"]
  }
}
